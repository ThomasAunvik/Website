{
  "plugins": ["jsx-a11y", "unused-imports"],
  "extends": ["next/core-web-vitals", "prettier", "plugin:jsx-a11y/strict"],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
