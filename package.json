    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.28.6",
    "eslint": "8.51.0",
    "eslint-config-next": "^13.5.6",
    "next": "^13.5.6",
    "next-auth": "^0.0.0-manual.dacbe24d",
    "postcss": "8.4.31",
    "postgres": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.69.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/react-fontawesome": "^1.6.7",
