{
  "name": "thaun-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint . --fix --config .eslintrc.json --ext .ts,.tsx",
    "eslint:files": "yarn eslint | grep -e '^/.*tsx$' -e '^/.*ts$'",
    "eslint:files:open": "code $(yarn --silent eslint:files)",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "node -r esbuild-register ./db/migrate.ts",
    "db:studio": "drizzle-kit studio --verbose",
    "updates": "npm-check-updates"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.4",
