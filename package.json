    "@vercel/postgres": "^0.5.1",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.29.0",
    "eslint": "8.53.0",
    "eslint-config-next": "^14.0.2",
    "lucide-react": "^0.292.0",
    "next": "^14.0.2",
    "next-auth": "experimental",
    "next-axiom": "^1.1.0",
